<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/javaCrash"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Java Crash" />

    <Button
        android:id="@+id/nativeCrash"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Native Crash" />

    <Button
        android:id="@+id/debug"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="remote debug" />

    <Button
        android:id="@+id/appmonitor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Appmonitor" />

    <Button
        android:id="@+id/telescope"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Event事件" />

</LinearLayout>